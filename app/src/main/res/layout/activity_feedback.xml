<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_feedback"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.umemory.FeedbackActivity">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="20sp"
        android:src="@drawable/logo_m"
        android:layout_gravity="center"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="10sp"
        android:text="意见反馈"
        android:gravity="center"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_margin="5sp"
        android:background="@color/textHintColor"/>
    <EditText
        style="@style/myEditText"
        android:id="@+id/f_email"
        android:background="@color/colorImage"
        android:hint="请填写邮箱"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_margin="5sp"
        android:background="@color/textHintColor"/>
    <EditText
        style="@style/myEditText"
        android:id="@+id/f_content"
        android:background="@color/colorImage"
        android:hint="@string/f_content"/>
</LinearLayout>
